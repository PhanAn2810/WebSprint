<script>
    function MemberRegister() {    
        //Khối chứa form
        var containerSelector = "#register-form";
        var $container = $(containerSelector);

        //Gửi dữ liệu lên server
        if ($container.validate('check')) {
            AjaxLoading(true);

            //Khóa nút gửi để không bị gửi nhiều lần     
            $("a[data-function='MemberRegister']").addClass("btn-loading");

            $.ajax({
                url: "/themes/theme01/ajax/member.aspx",
                type: "POST",
                dataType: "json",
                data: {
                    "action": "MemberRegister",
                    "hoTen": $("#tbHoVaTen_rgf").val(),
                    "dienThoai": $("#tbDienThoai_rgf").val(),
                    "email": $("#tbEmail_rgf").val(),
                    "diaChi": $("#tbDiaChi_rgf").val(),
                    "gioiTinh": $("input[type='radio'][name='rbGioiTinh_rgf']:checked", $container).val(),
                    "ngaySinh": $("#tbNgaySinh_rgf").val(),
                    "matKhau": $("#tbMatKhau_rgf").val()
                },
                success: function (res) {
                    if (res[0] === "success") {
                        AjaxLoading(false);

                        //Mở lại nút gửi
                        $("a[data-function='MemberRegister']").removeClass("btn-loading").text("Đăng ký thành công").css("pointer-events","none");

                        //Hiện thông báo cần kích hoạt tài khoản
                        $(".form-alert", $container).CreateFormAlert(res[1], true, true);
                    }
                    else {
                        AjaxLoading(false);

                        //Mở lại nút gửi                   
                        $("a[data-function='MemberRegister']").removeClass("btn-loading");

                        //Hiện thông báo lỗi
                        $(".form-alert", $container).CreateFormAlert(res[1], false, true);
                    }
                },
                error: function (error) {
                    console.log("Có lỗi xảy ra. MemberRegister");
                }
            });
        }
    }

    function CheckMemberLogin(event) {
        event.preventDefault();

        if (event.keyCode === 13)
            MemberLogin();
    }

    function MemberLogin() {
        //Khối chứa form
        var containerSelector = "#login-form";
        var $container = $(containerSelector);

        //Gửi dữ liệu lên server
        if ($container.validate('check')) {

            //Khóa nút gửi để không bị gửi nhiều lần     
            $("a[data-function='MemberLogin']").css("pointer-events", "none").find("span").text("Đang đăng nhập");

            $.ajax({
                url: "/themes/theme01/ajax/member.aspx",
                type: "POST",
                dataType: "json",
                data: {
                    "action": "MemberLogin",
                    "tenDangNhap": $("#tbTenDangNhap_lif").val(),
                    "matKhau": $("#tbMatKhau_lif").val()
                },
                success: function (res) {
                    if (res[0] === "success") {
                        //Hiện thông báo thành công
                        $(".form-alert", $container).CreateFormAlert(res[1], true);

                        //Chuyển tới trang thành viên
                        window.location = $("#hdRedirectUrl_lif").val();
                    }
                    else {
                        //Mở lại nút gửi                   
                        $("a[data-function='MemberLogin']").css("pointer-events", "auto").find("span").text("Đăng nhập");

                        //Hiện thông báo lỗi
                        $(".form-alert", $container).CreateFormAlert(res[1], false);
                    }
                },
                error: function (error) {
                    console.log("Có lỗi xảy ra. MemberLogin");
                }
            });
        }
    }

    function MemberForgotPassword() {
        //Khối chứa form
        var containerSelector = "#forgot-password-form";
        var $container = $(containerSelector);

        //Gửi dữ liệu lên server
        if ($container.validate('check')) {

            //Khóa nút gửi để không bị gửi nhiều lần     
            $("a[data-function='MemberForgotPassword']").css("pointer-events", "none").find("span").text("Đang đặt lại");

            $.ajax({
                url: "/themes/theme01/ajax/member.aspx",
                type: "POST",
                dataType: "json",
                data: {
                    "action": "MemberForgotPassword",
                    "email": $("#tbEmail_fpf").val()
                },
                success: function (res) {
                    if (res[0] === "success") {
                        //Hiện thông báo thành công
                        $(".form-alert", $container).CreateFormAlert(res[1], true);
                    }
                    else {
                        //Mở lại nút gửi                   
                        $("a[data-function='MemberForgotPassword']").css("pointer-events", "auto").find("span").text("Đặt lại mật khẩu");

                        //Hiện thông báo lỗi
                        $(".form-alert", $container).CreateFormAlert(res[1], false);
                    }
                },
                error: function (error) {
                    console.log("Có lỗi xảy ra. MemberForgotPassword");
                }
            });
        }
    }

    // #region Mở popup đăng ký, đăng nhập
    function OpenPopupRegister() {
        OpenPopup("#popup-login-register");

        $("#hdRedirectUrl_lif").val(document.URL);

        $("a[data-tab='tab-dangky']").click();

        $("#tbHoVaTen_rgf").focus();
    }

    function OpenPopupLogin() {
        OpenPopup("#popup-login-register");

        $("#hdRedirectUrl_lif").val(document.URL);

        $("a[data-tab='tab-dangnhap']").click();

        $("#tbTenDangNhap_lif").focus();
    }

    function OpenPopupForgotPassword() {
        OpenPopup("#popup-login-register");

        $("a[data-tab='tab-forgot']").click();

        $("#tbEmail_fpf").focus();
    }
// #endregion
</script>



